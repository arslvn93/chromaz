{
  "manifest_version": 3,
  "name": "My Chrome Extension",
  "description": "A simple Chrome extension built with React, TypeScript and Tailwind",
  "version": "1.0.0.0",
  "options_ui": {
    "page": "src/pages/options/index.html"
  },
  "action": {
    "default_popup": "src/pages/popup/index.html",
    "default_icon": {
      "32": "public/dev-icon-32.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "src/pages/newtab/index.html"
  },
  "icons": {
    "32": "public/dev-icon-32.png",
    "128": "public/dev-icon-128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "js": [
        "assets/index.tsx-loader-Cka7UI9V.js"
      ],
      "css": [
        "assets/tailwind-MvjZxDZY.css",
        "assets/index-CdnoCYH2.css"
      ]
    }
  ],
  "devtools_page": "src/pages/devtools/index.html",
  "web_accessible_resources": [
    {
      "resources": [
        "assets/*",
        "public/*"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}